{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@orm/model/(.*)": "<rootDir>/../libs/model/src/$1",
    "@orm/model": "<rootDir>/../libs/model/src",
    "@backend/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@backend/common": "<rootDir>/../libs/common/src",
    "@backend/bootstrap/(.*)": "<rootDir>/../libs/bootstrap/src/$1",
    "@backend/bootstrap": "<rootDir>/../libs/bootstrap/src",
    "@backend/security/(.*)": "<rootDir>/../libs/security/src/$1",
    "@backend/security": "<rootDir>/../libs/security/src",
    "@backend/gateway/(.*)": "<rootDir>/../libs/gateway/src/$1",
    "@backend/gateway": "<rootDir>/../libs/gateway/src"
  }
}